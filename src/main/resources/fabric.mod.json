{
	"schemaVersion": 1,
	"id": "testmod",
	"version": "${version}",
	"name": "Test Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://github.com/LCLPYT/fabric-mod-boilerplate",
		"sources": "https://github.com/LCLPYT/fabric-mod-boilerplate"
	},
	"license": "CC0-1.0",
	"icon": "assets/testmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.ExampleMod"
		],
		"client": [
			"com.example.ExampleModClient"
		],
		"fabric-datagen": [
			"com.example.ExampleModDataGenerator"
		]
	},
	"mixins": [
		"testmod.mixins.json",
		{
			"config": "testmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.7",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {}
